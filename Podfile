# platform :ios, '13.0'

target 'TestUnit' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for TestUnit

  target 'TestUnitTests' do
    inherit! :search_paths
    use_frameworks!
    # Pods for testing
    pod ‘Quick’
    pod ‘Nimble’
  end

  target 'TestUnitUITests' do
    # Pods for testing
    use_frameworks!
    pod ‘Quick’
    pod ‘Nimble’
  end

end
